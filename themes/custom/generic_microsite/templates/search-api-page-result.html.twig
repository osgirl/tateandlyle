{#
/**
 * @file
 * Default theme implementation for displaying a single search result coming from Search API page.
 *
 * @ingroup themeable
 */
#}
<div class="search-result">
	{{ title_prefix }}
	<h4{{ title_attributes }}>
	  <a href="{{ url }}">{{ title }}</a>
	</h4>
	{{ title_suffix }}
	{% if snippet %}
	  <p{{ content_attributes }}>{{ snippet }}</p>
	{% endif %}
	{% if info %}
	  <p>{{ info }}</p>
	{% endif %}
</div>
